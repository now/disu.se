<% if constant_list.empty? %>
<section>
  <h1>Constants</h1>
<% end %>
  <% @inherited_constants.each do |superclass, constants| %>
  <p class="inherited">Constants <%= superclass.type == :class ? 'inherited' : 'included' %> from <%= linkify(superclass) %>:</p>
  <ul>
  <% constants.sort_by{ |e| e.name.to_s }.each do |constant| %>
    <li><p><%= linkify(constant, constant.name) %></p></li>
  <% end %>
  </ul>
  <% end %>
<% if constant_list.empty? %>
</section>
<% end %>
