<section id="constructor_details" class="method_details_list">
  <h1>Constructor Details</h1>
  <% if @constructor.namespace != object %>
    <p class="notice">This class inherits a constructor from <%= linkify(@constructor, @constructor.namespace.path) %></p>
  <% else %>
    <%= yieldall :object => @constructor %>
  <% end %>
</section>
